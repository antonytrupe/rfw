{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "dist",
    "lib": [
      "es2019"
    ],
    "target": "es2019",
    "isolatedModules": false,
    "noEmit": false,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "server.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ]
}